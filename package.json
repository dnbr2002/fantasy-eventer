{
  "name": "Fantasy-Eventer",
  "version": "0.6.0",
  "description": "Fantasy Eventer Sports React App",
  "homepage": "https://fantasy-eventer.firebaseapp.com",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Bryce Kinnamon",
    "email": "bryce.kinnamon@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1.4"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "build": "run-s build:css build:js",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build:js": "cross-env NODE_PATH=. react-scripts build",
    "start": "run-p start:css start:js",
    "start:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "cross-env NODE_PATH=. react-scripts start",
    "test": "cross-env NODE_PATH=. react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true NODE_PATH=. react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^1.7.2",
    "@devexpress/dx-react-grid": "^1.7.2",
    "@devexpress/dx-react-grid-material-ui": "^1.7.2",
    "@material-ui/core": "^1.0.0-rc.0",
    "@material-ui/icons": "^1.0.0-beta.43",
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.1.1",
    "bower": "^1.8.4",
    "classnames": "^2.2.5",
    "firebase": "^4.1.3",
    "history": "^4.6.3",
    "hoek": "^5.0.3",
    "immutable": "^3.8.1",
    "lodash": "^4.17.10",
    "material-ui": "^1.0.0-beta.47",
    "material-ui-password-field": "^2.0.1",
    "mudash": "^0.10.0",
    "prop-types": "^15.5.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.0.10",
    "react-social-login-buttons": "^2.1.2",
    "recompose": "^0.27.1",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "firebase-tools": "^3.9.1",
    "flow-bin": "^0.89.0",
    "minx": "r-park/minx.git",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8"
  }
}
