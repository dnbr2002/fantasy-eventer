{
  "name": "Fantasy-Eventer",
  "version": "0.6.0",
  "description": "Fantasy Eventer Sports React App",
  "homepage": "https://fantasy-eventer.firebaseapp.com",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Bryce Kinnamon",
    "email": "bryce.kinnamon@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1.4"
  },
  "scripts": {
    "eject": "react-scripts eject",
    "build": "run-s build:css build:js",
    "build:css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build:js": "cross-env NODE_PATH=. react-scripts build",
    "start": "run-p start:js",
    "start:css": "npm run build:css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start:js": "cross-env NODE_PATH=. react-scripts start",
    "test": "cross-env NODE_PATH=. react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true NODE_PATH=. react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@devexpress/dx-react-core": "^1.11.1",
    "@devexpress/dx-react-grid": "^1.11.1",
    "@devexpress/dx-react-grid-material-ui": "^1.11.1",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/styles": "^4.0.0",
    "bootstrap": "^3.3.7",
    "bootstrap-social": "^5.1.1",
    "bower": "^1.8.4",
    "classnames": "^2.2.5",
    "firebase": "^5.2.0",
    "history": "^4.6.3",
    "hoek": "^5.0.3",
    "immutable": "^3.8.1",
    "lodash": "^4.17.10",
    "mudash": "^0.10.0",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-perfect-scrollbar": "^1.5.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "3.0.0",
    "react-social-login-buttons": "^2.1.2",
    "react-sortable-hoc": "^1.9.1",
    "recompose": "^0.27.1",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "rxjs": "^6.5.2",
    "toastr": "^2.1.4",
    "underscore": "^1.9.1",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "enzyme": "^2.9.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "firebase-tools": "^3.19.3",
    "minx": "github:r-park/minx",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
